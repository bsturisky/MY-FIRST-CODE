var hello = prompt('What is your name?');
if(hello=='lauren')
{
     alert('give me the debit card now');
} 
 else if(hello=='hilton')
{   
     alert('how is Crawford doing?');
}
 else if(hello=='elliot')
{    
     alert('code something better then this I dare you');
}     
 else if(hello=='benjamin')
{
     alert('we are gonna get smacked by Milton');
}
 else if(hello=='jared')
{
     alert('study for your act')
}
